CONFIG_ESP_SYSTEM_MEMPROT_FEATURE=n
CONFIG_FREERTOS_UNICORE=y
CONFIG_MAGNOLIA_SCHED_SELFTESTS=n
CONFIG_MAGNOLIA_JOB_SELFTESTS=n
CONFIG_MAGNOLIA_ALLOC_SELFTESTS=n
# Ensure IPC SHM can provide enough regions for DevFS streaming devices.
CONFIG_MAGNOLIA_IPC_MAX_SHM_REGIONS=16
# Leave headroom for runtime/devfs self-test registrations.
CONFIG_MAGNOLIA_DEVFS_MAX_DEVICES=24

# Userland ELF (init + applets) needs more stack than ESP-IDF defaults.
CONFIG_MAGNOLIA_JOB_WORKER_STACK_DEPTH=8192
CONFIG_MAGNOLIA_ELF_INIT_STACK_DEPTH=65536
